.head {
  width: 100%;
  height: 440rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.head_img {
  width: 461rpx;
  height: 296rpx;
  background-color: red;
}

.editors {
  width: 230rpx;
  height: 60rpx;
  border: 1rpx solid rgba(74, 144, 226, 1);
  border-radius: 5rpx;
  margin-top: 26rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 60rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(74, 144, 226, 1);
}

/* pages/Release/push_experience/push_experience.wxss */
@import "../../../template/template.wxss";

.kong {
  width: 100%;
  height: 150rpx;
}

.produce {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 25rpx;
}

.produce_title {
  font-size: 32rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
}

.produce_main {
  width: 100%;
  height: 200rpx;
  box-sizing: border-box;
  padding: 25rpx 0rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
}

.active_title {
  margin-top: 16rpx;
  height: 88rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 0rpx 25rpx;
  font-size: 32rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
}
