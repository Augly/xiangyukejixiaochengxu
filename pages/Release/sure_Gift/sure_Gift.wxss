/* pages/Release/sure_Gift/sure_Gift.wxss */
.giftList{
  width: 100%;
  height: 218rpx;
  border-bottom: 2rpx solid RGBA(235, 235, 235, 1);
  box-sizing:border-box;
  padding: 24rpx 33rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.list_logo{
  width: 170rpx;
  height: 170rpx;
  box-sizing: border-box;
  border: 1rpx solid RGBA(235, 235, 235, 1);
}

.list_res{
  width: 400rpx;
  height: 170rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}

.buy_group{
  width: 210rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.jian,.jia{
  width: 60rpx;
  height: 60rpx;
  border: 1rpx solid RGBA(235, 235, 235, 1);
  background-color: RGBA(245, 245, 245, 1);
  border-radius:3rpx;
  display: flex;
  align-items: center;
  justify-content: center; 
}

.buy_group .iconfont{
  font-size: 30rpx;
}
.res_title{
  font-size:36rpx;
font-family:PingFang-SC-Medium;
color:rgba(0,0,0,1);
}
.show{
  font-size:36rpx;
  text-align: center;
  line-height: 60rpx;
font-family:PingFang-SC-Regular;
color:rgba(51,51,51,1);
}
.cendel{
  font-size:24rpx;
font-family:PingFang-SC-Medium;
color:rgba(255,0,0,1);
align-self: flex-start;
}