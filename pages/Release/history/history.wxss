/* pages/Release/history/history.wxss */
@import "../../../template/template.wxss";

.mywrap {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}

.mywrap_head {
  width: 100%;
  height: 114rpx;
  box-sizing: border-box;
  padding: 0rpx 24rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.cendel, .mypush {
  width: 171rpx;
  height: 72rpx;
  border: 2rpx solid RGBA(152, 97, 225, 1);
  border-radius: 5rpx;
  text-align: center;
  line-height: 72rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(152, 97, 225, 1);
}
