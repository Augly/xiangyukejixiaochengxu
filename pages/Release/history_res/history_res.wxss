/* pages/Release/history_res/history_res.wxss */
/* pages/Release/experience_res/experience_res.wxss */
@import "../../../template/template.wxss";

.wrap {
  width: 100%;
  height: auto;
  font-size: 36rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(0, 0, 0, 1);
  box-sizing: border-box;
  padding: 0rpx 32rpx;
}

.head_title {
  font-weight: bold;
}

.anthur_Group {
  width: 100%;
  display: flex;
  align-items: center;
  margin-top: 35rpx;
}

.anthur_logo {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.anthur_nameGroup {
  width: 400rpx;
  height: 100rpx;
  margin-left: 17rpx;
}

.name {
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
}

.icon-nv {
  font-size: 30rpx;
  color: RGBA(255, 61, 201, 1);
}

.data {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
}

.zan {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
}

.icon-xihuan {
  font-size: 30rpx;
  margin-right: 5rpx;
}

.operation {
  width: 150rpx;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.gz, .sc {
  width: 150rpx;
  height: 48rpx;
  border: 1rpx solid RGBA(204, 204, 204, 1);
  border-radius: 25rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
  display: flex;
  align-items: center;
  justify-content: center;
}

.gzed {
  color: RGBA(152, 97, 225, 1);
}

.icon-guanzhu, .icon-collection {
  font-size: 30rpx;
  margin-right: 8rpx;
}

.icon-collection-b {
  font-size: 30rpx;
  margin-right: 8rpx;
  color: RGBA(255, 246, 0, 1);
}

.content {
  margin-top: 33rpx;
}

.foot {
  width: 100%;
  height: 100rpx;
  border-top: 1rpx solid RGBA(0, 0, 0, 0.12);
  background-color: rgba(255, 255, 255, 1);
  position: fixed;
  bottom: 0;
  display: flex;
}

.foot_item {
  width: 25%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
}

.foot_item .iconfont {
  font-size: 30rpx;
  display: block;
}

.zaned {
  color: RGBA(152, 97, 225, 1);
}

.icon-xihuan1 {
  font-size: 30rpx;
  margin-right: 5rpx;
  color: RGBA(152, 97, 225, 1);
}

.giftGroup {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background-color: rgba(51, 51, 51, 0.8);
}

.giftTab {
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  padding: 0rpx 20rpx;
  display: flex;
  align-items: center;
}

.giftList {
  position: absolute;
  bottom: 0;
  width: 100%;
  background-image: url('http://xiangyu.wx.bronet.cn/images/tanbox@2x.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}

.Tab_item {
  width: 190rpx;
  height: 35rpx;
  border-right: 1rpx solid RGBA(199, 166, 242, 1);
  font-size: 30rpx;
  text-align: center;
  font-family: PingFang-SC-Medium;
  color: RGBA(199, 166, 242, 1);
  line-height: 35rpx;
}

.giftscrollView {
  width: 100%;
  height: 400rpx;
}

.send {
  width: 160rpx;
  height: 60rpx;
  background: rgba(28, 201, 221, 1);
  border-radius: 5rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 60rpx;
}

.list_item {
  width: 25%;
  height: 200rpx;
  float: left;
}

.item_gift {
  width: 140rpx;
  height: 140rpx;
  margin: 0 auto;
  box-sizing: border-box;
}

.Tab_itemActive {
  color: RGBA(28, 201, 221, 1);
}

.item_gift image {
  width: 100%;
  height: 100%;
}

.item_giftActive {
  border: 1rpx solid RGBA(35, 190, 174, 1);
  border-radius: 10rpx;
  background: rgba(16, 39, 102, 1);
}

.item_title {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 50rpx;
}

.noGift {
  width: 680rpx;
  height: 400rpx;
  background-color: rgba(255, 255, 255, 1);
  border-radius: 20rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -200rpx;
  margin-left: -340rpx;
  overflow: hidden;
}

.notice_res {
  width: 100%;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.notice_res_title {
  font-size: 36rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(0, 0, 0, 1);
}

.notice_res_main {
  font-size: 28rpx;
  margin-top: 50rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
}

.notice_Group {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  font-size: 36rpx;
  font-family: PingFang-SC-Medium;
}

.cendel, .goshop {
  width: 50%;
  height: 100%;
  text-align: center;
  line-height: 100rpx;
}

.cendel {
  background-color: RGBA(243, 243, 243, 1);
  color: RGBA(153, 153, 153, 1);
}

.goshop {
  background-color: RGBA(74, 144, 226, 1);
  color: RGBA(255, 255, 255, 1);
}

.tabbar {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  border-top: 8rpx solid RGBA(245, 246, 250, 1);
  border-bottom: 2rpx solid RGBA(245, 246, 250, 1);
}

.tabbar_item {
  width: 33%;
  height: 100%;
}

.title {
  width: 80rpx;
  height: 100%;
  box-sizing: border-box;
  font-size: 32rpx;
  line-height: 100rpx;
  text-align: center;
  font-family: PingFang-SC-Medium;
  margin: 0 auto;
}

.titleActive {
  color: rgba(152, 97, 225, 1);
  border-bottom: 3rpx solid RGBA(152, 97, 225, 1);
}

.seacherGroup {
  width: 686rpx;
  height: 88rpx;
  border: 1rpx solid RGBA(235, 235, 235, 1);
  border-radius: 5rpx;
  margin: 0 auto;
  margin-top: 16rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0rpx 22rpx;
}

.icon-sousuo {
  font-size: 44rpx;
  color: RGBA(153, 153, 153, 1);
}

.seach_ipt {
  width: 500rpx;
  height: 98%;
  font-size: 32rpx;
  font-family: PingFang-SC-Medium;
}

.seacherTitle {
  font-size: 32rpx;
  font-family: PingFang-SC-Medium;
  line-height: 88rpx;
  color: RGBA(153, 153, 153, 1);
}

.kong{
  width: 100%;
  height: 150rpx;
}
