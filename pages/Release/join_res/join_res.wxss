/* pages/Release/join_res/join_res.wxss */
@import "../../../template/template.wxss";

.wrap {
  width: 100%;
  height: auto;
  font-size: 36rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(0, 0, 0, 1);
  box-sizing: border-box;
  padding: 0rpx 32rpx;
}

.head_title {
  font-weight: bold;
}

.anthur_Group {
  width: 100%;
  display: flex;
  align-items: center;
  margin-top: 35rpx;
}

.anthur_logo {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.anthur_nameGroup {
  width: 400rpx;
  height: 100rpx;
  margin-left: 17rpx;
}

.name {
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
}

.icon-nv {
  font-size: 30rpx;
  color: RGBA(255, 61, 201, 1);
}

.data {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
}

.send_Adder {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
}

.icon-xihuan {
  font-size: 30rpx;
  margin-right: 5rpx;
}

.operation {
  width: 150rpx;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.gz, .sc {
  width: 150rpx;
  height: 48rpx;
  border: 1rpx solid RGBA(204, 204, 204, 1);
  border-radius: 25rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
  display: flex;
  align-items: center;
  justify-content: center;
}

.gzed {
  color: RGBA(152, 97, 225, 1);
}

.icon-guanzhu, .icon-collection {
  font-size: 30rpx;
  margin-right: 8rpx;
}

.icon-collection-b {
  font-size: 30rpx;
  margin-right: 8rpx;
  color: RGBA(255, 246, 0, 1);
}

.content {
  margin-top: 33rpx;
}

.allpl {
  width: 100%;
  height: 64rpx;
  box-sizing: border-box;
  padding: 0rpx 33rpx;
  font-size: 24rpx;
  line-height: 64rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
  background-color: RGBA(238, 238, 238, 1);
}

.send_group {
  width: 100%;
  height: 100rpx;
  background-color: rgba(255, 255, 255, 1);
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 0rpx 32rpx;
  justify-content: space-between;
}

.send_ipt {
  width: 458rpx;
  height: 60rpx;
  border-bottom: 1rpx solid RGBA(66, 87, 122, 1);
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  color: RGBA(153, 153, 153, 1);
}

.send_sc, .send_share {
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  background-color: RGBA(132, 148, 173, 1);
  display: flex;
  align-items: center;
  justify-content: center;
}

.icon-xihuan {
  font-size: 40rpx;
  color: rgba(255, 255, 255, 1);
}

.icon-fenxiang {
  font-size: 40rpx;
  color: rgba(255, 255, 255, 1);
}

.send_groupone {
  width: 100%;
  height: 100rpx;
  background-color: rgba(255, 255, 255, 1);
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 20rpx 32rpx;
  justify-content: space-between;
}

.iptGroup {
  width: 100%;
  height: 100%;
  border-bottom: 1rpx solid rgba(51, 51, 51, 1);
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.ipt_Group {
  width: 580rpx;
  height: 100%;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
}

.Send {
  display: flex;
  align-items: center;
  line-height: 60rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(66, 87, 122, 1);
}

.icon-fabu {
  font-size: 30rpx;
  margin-right: 10rpx;
  color: rgba(51, 51, 51, 1);
}

.mask {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background-image: url('http://xiangyu.wx.bronet.cn/images/bbg@2x.png');
  background-size: 100% 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.main_userGroup {
  width: 100%;
  height: 480rpx;
  display: flex;
  align-items: center;
  margin-top: 80rpx;
  box-sizing: border-box;
  padding: 0rpx 35rpx;
}

.mask_main {
  width: 600rpx;
  height: 723rpx;
  border-radius: 50rpx;
  background-color: red;
  background-image: url('http://xiangyu.wx.bronet.cn/images/bbg01@2x.png');
  background-size: 100% 100%;
}

.perSonl {
  position: relative;
  width: 300rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.per_sonl {
  width: 220rpx;
  height: 220rpx;
  border-radius: 50%;
}

.sex {
  width: 64rpx;
  height: 64rpx;
  position: absolute;
  background-color: rgba(255, 255, 255, 1);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  top: 250rpx;
  left: 60%;
}

.per_title {
  font-size: 40rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  margin-top: 30rpx;
}

.startZuo {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.main_button {
  display: flex;
  align-items: center;
  justify-content: center;
}

.changeCard, .sendGift {
  width: 210rpx;
  height: 80rpx;
  border-radius: 40rpx;
  font-size:30rpx;
  text-align: center;
  line-height: 80rpx;
font-family:MicrosoftYaHei;
color:rgba(202,0,252,1);
  background: linear-gradient(to right, RGBA(254, 217, 51, 1), RGBA(252, 243, 52, 1));
}

.icon-baiyangzuo {
  font-size: 120rpx;
  color: rgba(255, 255, 255, 1);
}

.startTitle {
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(255, 255, 255, 1);
}
.main_botton{
  box-sizing: border-box;
  padding: 0rpx 50rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 42rpx;
}