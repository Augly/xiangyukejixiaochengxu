page {
  width: 100%;
  height: 100%;
  background-color: RGBA(245, 246, 250, 1);
}

.upload_imgGroup {
  box-sizing: border-box;
  padding: 32rpx;
  background-color: rgba(255, 255, 255, 1);
}

.upload_imgGroup_title {
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
}

.upload_wrap {
  display: flex;
  flex-wrap: wrap;
}

.upload_item {
  width: 209rpx;
  height: 209rpx;
  border-radius: 5rpx;
  margin-right: 19rpx;
  display: block;
  margin-top: 19rpx;
  position: relative;
}

.mytitle {
  background-color: rgba(255, 255, 255, 1);
  margin-top: 15rpx;
  box-sizing: border-box;
  padding: 39rpx;
  display: flex;
}

.title_t {
  width: 180rpx;
  height: auto;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
}

.title_text {
  width: 550rpx;
  height: 180rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(102, 102, 102, 1);
}

.img_cha {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 0;
  right: 0;
  background-image: url('http://xiangyu.wx.bronet.cn/images/xiao.png');
  background-size: 100% 100%;
}

.userItem {
  width: 100%;
  height: 110rpx;
  border-bottom: 1rpx solid RGBA(235, 235, 235, 1);
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 0rpx 36rpx;
}

.userInfo {
  background-color: rgba(255, 255, 255, 1);
  margin-top: 16rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
}

.item_title {
  width: 150rpx;
  color: rgba(51, 51, 51, 1);
}

.item_ipt {
  width: 500rpx;
  height: 98rpx;
  color: RGBA(102, 102, 102, 1);
}
.item_iptView{
  line-height: 98rpx;
  color: rgba(0, 0, 0, 1);
}

.kong {
  width: 100%;
  height: 140rpx;
}

.footBottom {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background: rgba(74, 144, 226, 1);
  font-size: 36rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 100rpx;
}
