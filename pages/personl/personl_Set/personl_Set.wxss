page {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: RGBA(245, 246, 250, 1);
}

.myset {
  width: 100%;
  height: 250rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(255, 255, 255, 1);
}

.myset_logo {
  width: 180rpx;
  height: 180rpx;
  box-sizing: border-box;
  border-radius: 50%;
  box-shadow: 0rpx 0rpx 2rpx rgba(51, 51, 51, 1);
  background-size: 100% 100%;
  position: relative;
}

.logo_wrap {
  width: 180rpx;
  height: 180rpx;
  box-sizing: border-box;
  border-radius: 50%;
  position: absolute;
  background-color: rgba(0, 0, 0, 0.3);
  display: flex;
  align-items: center;
  justify-content: center;
}

.icon-camera2 {
  font-size: 65rpx;
  color: rgba(255, 255, 255, 1);
}

.myzl {
  width: 100%;
  margin-top: 16rpx;
  background-color: rgba(255, 255, 255, 1);
}

.myzl_item {
  width: 100%;
  height: 100rpx;
  border-bottom: 1rpx solid RGBA(235, 235, 235, 1);
  display: flex;
  align-items: center;
  justify-content: flex-end;
  box-sizing: border-box;
  padding: 0rpx 22rpx;
  line-height: 100rpx;
}

.myzl_item_title {
  width: 300rpx;
  height: 100%;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
  margin-right: auto;
}

.myzl_item_mintitle {
  /* width: 200rpx; */
  height: 100%;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(0, 0, 0, 1);
}

.icon-bangzhujinru {
  font-size: 28rpx;
  margin-left: 40rpx;
  color: RGBA(153, 153, 153, 1);
}
